    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.loot8.loot8_app",
      "permissions": "com.android.vending.BILLING"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "55943958-8ea8-4990-a472-3e4386fefdbd"
      }
    },
    "owner": "kshatriyan"
  }
}
